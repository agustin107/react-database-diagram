{
    "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
    "linterOptions": {
      "exclude": ["node_modules/**/*.ts"]
    },
    "rules": {
      "no-console": false,
      "object-literal-sort-keys": false,
      "no-string-literal": false,
      "ordered-imports": { "severity": "default" }
    },
    "defaultSeverity": "warning"
  }
  